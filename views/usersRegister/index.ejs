<h2 class="center">UsersRegister Table</h2>
<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    </thead>
    <tbody>
    <% usersRegister.forEach(registerUser => { %>
        <tr>
            <td>
                <a href="<%= `/usersRegister/${registerUser._id}` %>">
                    <%= usersRegister.name %>
                </a>
            </td>
            <td><%= usersRegister.email %></td>
            <td>
                <a href="<%= `usersRegister/${registerUser._id}/edit` %>">
                    Edit
                </a>
            </td>
            <td>
                <a href="<%= `usersRegister/${registerUser._id}/delete?_method=DELETE` %>"
                   âž¥onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
            </td>
        </tr>
    <% }); %>
    </tbody>
</table>


})
